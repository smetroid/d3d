{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "chrome",
  "documentRoot": "/frontend/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "port": 18067,
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "modes": {
    "window": {
      "title": "D3 Diagrams",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/frontend/src/assets/cmdr_logo.png"
    },
    "chrome": {
      "title": "D3 Diagrams",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/frontend/src/assets/cmdr_logo.png"
    }
  },
  "cli": {
    "binaryName": "d3d",
    "resourcesPath": "/frontend/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/frontend/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary": {
      "patchFile": "/frontend/index.html",
      "devUrl": "http://localhost:8080"
    }
  }
}